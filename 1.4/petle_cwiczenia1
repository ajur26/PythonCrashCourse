lista = []
for number in range(1,1000001):
    lista.append(number)
print(lista)
print(min(lista))
print(max(lista))
print(sum(lista))

odd_numbers = list(range(1,21))

for number in odd_numbers:
    if number % 2 !=0:
        print(number)

for number in range(3,31):
    print(number**2)

squares = [value**2 for value in range(3,31)]
print(squares)

value3 = [value**3 for value in range(1,10)]
print(value3)